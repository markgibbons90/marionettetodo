{
  "name": "marionettetodo",
  "version": "0.0.0",
  "description": "A to-do application built with Backbone.Marionette",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-scss": "node-sass -r css -o css",
    "watch-scss": "node-sass -rw css -o css",
    "hbs": "handlebars js/templates/* -f js/templates/compiled/compiled.js",
    "watch-hbs": "watch 'npm run hbs' js/templates"
  },
  "author": "Mark Gibbons",
  "license": "BSD-2-Clause",
  "dependencies": {
    "backbone.marionette": "~2.4.5",
    "jquery": "~1.12.2",
    "bootstrap-sass": "~3.3.6",
    "handlebars": "~4.0.5"
  },
  "devDependencies": {
    "node-sass": "~3.4.2",
    "watch": "~0.17.1"
  }
}
